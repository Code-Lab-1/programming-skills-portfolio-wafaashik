def make_album(artist, title):
    album_dict = {
        'artist': artist.title(),
        'title': title.title(),
        }
    return album_dict

album = make_album('Taylor Swift', 'Midnights')
print(album)

album = make_album('IU', 'Palette')
print(album)

album = make_album('Selena Gomez', 'Rare')
print(album)