favorite_places = {
    'Azra': ['Hawaii', 'Maldives', 'France'],
    'Luna': ['Paris', 'Korea','Japan'],
    'John': ['Australia', 'Germany', 'Turkey']
    }

for name, places in favorite_places.items():
    print(f"\n{name.title()} likes the following places:")
    for place in places:
        print(f"- {place.title()}")